{
    "name": "shamaseen/laravel-ratchet",
    "description": "laravel websocket using ratchet",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "shamaseen",
        "laravel-ratchet",
        "websocket",
        "ratchet",
        "php"
    ],
    "authors": [
        {
            "name": "Mohammad Shamaseen",
            "email": "m.shamaseen@outlook.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Shamaseen\\Laravel\\Ratchet\\LaravelRatchetServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Shamaseen\\Laravel\\Ratchet\\": "src/"
        }
    },

    "minimum-stability": "stable",
    "prefer-stable": true,

    "require": {
        "illuminate/session": ">5.7",
        "illuminate/support": ">5.7",
        "illuminate/console": ">5.7",
        "illuminate/validation": ">5.7",
        "react/zmq": "^0.4.0",
        "cboden/ratchet": ">0.4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": ">2.14",
        "sebastian/phpcpd": ">4.1",
        "phploc/phploc": ">4.0",
        "phpmd/phpmd": ">2.6"
    }
}
